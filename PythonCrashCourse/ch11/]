import unittest
from Employee import Employee

class TestEmployee(unittest.TestCase):
    def setUp(self): 
        self.name = 'Pawat'
        self.last = 'Biggy'
        self.a_salary = 5000
        self.custom = 7000
    
    def test_give_default_raise(self)
        my_employee = Employee(self.name, self.last, self.a_salary)
        my_employee.give_raise()
        self.assertIn(10000, my_employee.a_salary)
